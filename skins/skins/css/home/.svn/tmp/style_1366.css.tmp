@charset "utf-8";
/* 公共区 */
body {
	padding: 0px;
	margin: 0px;
	background-color: #090a1e;
	font-family: "microsoft yahei";
}

a {
	text-decoration: none;
}

a:hover {
	color: #333
}

:focus {
	outline: none;
}

::-moz-focus-inner {
	border-color: transparent;
}

i {
	font-style: normal;
}

p,h2,h3 {
	padding: 0;
	margin: 0;
}

h2,h3 {
	font-weight: 500;
}

.clearfix:after {
	content: ".";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
}

.clearfix {
	display: inline-block;
}

.clearfix {
	display: block;
}

input,textarea,select {
	outline: none;
	resize: none;
	font-size: 1em;
	vertical-align: middle;
}

ul,li {
	list-style: none outside none;
	margin: 0;
	padding: 0;
	position: relative;
}

@font-face {
	font-family: "方正正中黑简体";
	src: url("方正正中黑简体.ttf");
}

@font-face {
	font-family: "方正兰亭黑简";
	src: url("FZLTHJW.ttf");
}

.filter_box,.dw_side span,.dh_nav a {
	font-family: "方正正中黑简体";
}

.dw_side span b,.jkdpjbox p,.jkdpjbox>div ul li a,#main_box,.son_ul li,.inp_c,.dhpcbox h4 i,#ple ul li a
	{
	font-family: "方正兰亭黑简";
}

@font-face {
	font-family: "方正正大黑简体";
	src: url("方正正大黑简体.ttf");
}

.nav li a,.menu_box a,.jkdpj h2,.gzpj h2,.b_con h2,.dh_con h2,.ctbcon li:first-child span,.dhbcon li:first-child span,.gzlist h2,.dsjkbox h2 i,.dsjkbox h3,.dspcjkmin h4,.gzpic p b
