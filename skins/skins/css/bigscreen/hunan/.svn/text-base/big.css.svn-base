.pulist {
	padding: 100px 0 0 20px;
	overflow: hidden;
}

.pulist p {
	width: 260px;
	float: left;
	font-size: 1.35em;
	color: #fff;
	margin-bottom: 8px;
	height: 40px;
	overflow: hidden;
}

.pulist p i {
	width: 40px;
	height: 26px;
	border-radius: 2px;
	border: 1px #f6ab00 solid;
	background: #f6ab00;
	text-align: center;
	float: left;
	margin: 2px 12px 0 0;
}

.pulist p+p i {
	border: 1px #ed6d00 solid;
	background: #ed6d00;
}

.pulist p+p+p i {
	border: 1px #a9cf52 solid;
	background: #a9cf52;
}

.pulist p+p+p+p i {
	border: 1px #008974 solid;
	background: #008974;
}

.pulist p+p+p+p+p i {
	border: 1px #6c9bd2 solid;
	background: #6c9bd2;
}

.pulist p+p+p+p+p+p i {
	border: 1px #7fbe25 solid;
	background: #7fbe25;
}

.pulist p+p+p+p+p+p+p i {
	border: 1px #7f1085 solid;
	background: #7f1085;
}

.pulist p+p+p+p+p+p+p+p i {
	border: 1px #e60012 solid;
	background: #e60012;
}

.pulist p+p+p+p+p+p+p+p+p i {
	border: 1px #009c84 solid;
	background: #009c84;
}

.list_e li {
	height: 946px;
	background: #0b1c2e;
	margin-bottom: 30px;
}

.list_e li h2 {
	width: 915px;
	height: 57px;
	background: url("../../../img/bigscreen/hunan/dimg13.png") no-repeat;
	color: #fff;
	font-size: 2.2em;
	font-weight: 600;
	line-height: 47px;
	text-align: center;
}

.list_e li dl {
	width: 860px;
	color: #fff;
	padding: 25px 0 0 33px;
}

.list_e li dl dt {
	width: 843px;
	height: 74px;
	background: #006e6b;
	position: relative;
	overflow: hidden;
}

.list_e li dl dt i {
	width: 273px;
	height: 37px;
	text-align: center;
	font-size: 1.4em;
	line-height: 37px;
	background: #006e6b;
	border-bottom: 1px #004ea1 solid;
	position: absolute;
	top: 0;
	right: 0;
}

.list_e li dl dt span {
	height: 64px;
	float: left;
	font-size: 1.4em;
	font-weight: 500;
	border-left: 1px #004ea1 solid;
	text-align: center;
}

.list_e li dl dt span:nth-child(1) {
	width: 230px;
	padding-top: 25px;
}

.list_e li dl dt span:nth-child(2) {
	width: 115px;
	padding-top: 11px;
}

.list_e li dl dt span:nth-child(3) {
	width: 100px;
	padding-top: 11px;
}

.list_e li dl dt span:nth-child(4) {
	width: 120px;
	padding-top: 25px;
}

.list_e li dl dt span:nth-child(5) {
	width: 90px;
	padding-top: 42px;
}

.list_e li dl dt span:nth-child(6) {
	width: 90px;
	padding-top: 42px;
}

.list_e li dl dt span:nth-child(7) {
	width: 90px;
	padding-top: 42px;
	border-left: 1px #004ea1 solid;
}

.list_e li dl dd {
	width: 865px;
	height: 767px;
	padding: 0;
	margin: 0;
	overflow: auto;
}

.list_e li dl dd p {
	height: 58px;
	padding: 0;
	line-height: 58px;
}

.list_e li dl dd span {
	height: 58px;
	float: left;
	font-size: 1.35em;
	font-weight: 500;
	border-left: 1px #004ea1 solid;
	text-align: center;
	border-bottom: 1px #004ea1 solid;
}

.list_e li dl dd span:nth-child(1) {
	width: 230px;
}

.list_e li dl dd span:nth-child(2) {
	width: 115px;
}

.list_e li dl dd span:nth-child(3) {
	width: 100px;
}

.list_e li dl dd span:nth-child(4) {
	width: 120px;
}

.list_e li dl dd span:nth-child(5) {
	width: 90px;
}

.list_e li dl dd span:nth-child(6) {
	width: 90px;
}

.list_e li dl dd span:nth-child(7) {
	width: 90px;
	border-right: 1px #004ea1 solid;
}

.list_d li {
	height: 458px;
	background: #0b1c2e;
	margin-bottom: 30px;
}

.list_d li h2 {
	width: 56px;
	height: 458px;
	float: left;
	background: url("../../../img/bigscreen/hunan/dimg12.png") no-repeat;
	color: #fff;
	font-size: 2.2em;
	font-weight: 600;
	line-height: 38px;
	display: flex;
	align-items: center;
	justify-content: space-around;
	flex-direction: column;
	writing-mode: tb-rl;
}

.list_d li span {
	width: 1776px;
	height: 458px;
	float: left;
}

.list_d li span img {
	padding: 30px 0 0 52px;
}

.list_e li dl dd::-webkit-scrollbar {
	width: 8px;
	height: 8px;
}

.list_e li dl dd::-webkit-scrollbar-thumb {
	border-radius: 10px;
	-webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);
	background: #d76d23;
}

.list_e li dl dd::-webkit-scrollbar-track {
	-webkit-box-shadow: inset 0 0 5px rgba(0, 0, 0, 0.2);
	border-radius: 10px;
	background: #009c84;
}
.list_e li dl dd p span {
	width: 100px;
	text-align: center;
	float: left;
	display: inline-block;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
	border-left: 1px #00468e solid;
	border-bottom: 1px #00468e solid;
}
/***文字居左***/
.list_e li dl dd p span.text_left {
	text-align: left;
}
/***数字居右***/
.list_e li dl dd p span.number_right {
	text-align: right;
}
