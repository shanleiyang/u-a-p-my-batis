<?xml version="1.0" encoding="gb2312" ?>
<!DOCTYPE taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN"
  "http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">
<taglib>
	<tlibversion>1.0</tlibversion>
	<jspversion>1.1</jspversion>
	<shortname>webui</shortname>
	<uri>/tags/web-ui</uri>
    <tag>
		<name>ui</name>
		<tagclass>com.nrjh.framework.taglib.SkinTag</tagclass>
		<bodycontent>JSP</bodycontent>
        <attribute>
			<name>img</name>
			<required>false</required>
		</attribute>
        <attribute>
			<name>css</name>
			<required>false</required>
		</attribute>
        <attribute>
			<name>js</name>
			<required>false</required>
		</attribute>
	</tag>
	<tag>
		<name>asset</name>
		<tagclass>com.nrjh.framework.taglib.AssetTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>path</name>
			<required>false</required>
		</attribute>
	</tag>
	<tag>
		<name>mediacss</name>
		<tagclass>com.nrjh.framework.taglib.MediaCssTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>path</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<name>skincontext</name>
		<tagclass>com.nrjh.framework.taglib.ContextTag</tagclass>
	</tag>
	<tag>
		<name>orgSelect</name>
		<tagclass>com.nrjh.framework.taglib.OrgSelectTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<attribute>
			<name>id</name>
			<rtexprvalue>true</rtexprvalue>
			<required>true</required>
		</attribute>
		<attribute>
			<name>name</name>
			<rtexprvalue>true</rtexprvalue>
			<required>false</required>
		</attribute>
		<attribute>
			<name>isLink</name>
			<rtexprvalue>true</rtexprvalue>
			<required>false</required>
		</attribute>
		<attribute>
			<name>childrenID</name>
			<rtexprvalue>true</rtexprvalue>
			<required>false</required>
		</attribute>
		<attribute>
			<name>parentID</name>
			<rtexprvalue>true</rtexprvalue>
			<required>false</required>
		</attribute>
		<attribute>
			<name>area</name>
			<rtexprvalue>true</rtexprvalue>
			<required>false</required>
		</attribute>
		<attribute>
			<name>styleClass</name>
			<rtexprvalue>true</rtexprvalue>
			<required>false</required>
		</attribute>
		<attribute>
			<name>style</name>
			<rtexprvalue>true</rtexprvalue>
			<required>false</required>
		</attribute>
		<attribute>
			<name>onclick</name>
			<rtexprvalue>true</rtexprvalue>
			<required>false</required>
		</attribute>
		<attribute>
			<name>onchange</name>
			<rtexprvalue>true</rtexprvalue>
			<required>false</required>
		</attribute>
	</tag>
	
</taglib>	
	